  <spring:url value="/resources/layout/js/fullcalendar.min.js" var="fullcalendarjs_url" />
  <spring:url value="/resources/layout/js/unicorn.js" var="unicornjs_url" />
  <spring:url value="/resources/layout/js/unicorn.dashboard.js" var="dashboard_url" />
  
    <spring:url value="/resources/layout/jquerytable/jquery.tablesorter.js" var="tablejs_url" />
  

  <script language="JavaScript" src="${excanvas_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>
  
