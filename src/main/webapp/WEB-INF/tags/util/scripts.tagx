  <spring:url value="/resources/layout/css/bootstrap-responsive.min.css" var="bootstrapresponsive_url" />
  
  <!-- TEMPLATE -->
  
  <spring:url value="/resources/layout/css/unicorn.main.css" var="unicorn_url" />
  <spring:url value="/resources/layout/css/unicorn.grey.css" var="unicornlayout_url" />
  
  <spring:url value="/resources/layout/jquerytable/blue/style.css" var="table_url" />
  

  <!-- FULLCALENDAR -->
  <spring:url value="/resources/layout/css/fullcalendar.css" var="fullcalendar_url" />
  
  <link rel="stylesheet" type="text/css" href="${bootstrap_url}" />  
  <link rel="stylesheet" type="text/css" href="${bootstrapresponsive_url}" />  
  <link rel="stylesheet" type="text/css" media="screen" href="${unicorn_url}" />  
  <link rel="stylesheet" type="text/css" href="${unicornlayout_url}" />
  <link rel="stylesheet" type="text/css" href="${fullcalendar_url}" />
  <link rel="stylesheet" type="text/css" href="${table_url}" />
  
  <!-- JS -->  
  <spring:url value="/resources/layout/js/excanvas.min.js" var="excanvas_url" />
  <spring:url value="/resources/layout/js/jquery.min.js" var="jquery_url" />
  <spring:url value="/resources/layout/js/jquery.ui.custom.js" var="jqueryui_url" />
  <spring:url value="/resources/layout/js/bootstrap.min.js" var="bootstrapjs_url" />  
  
  
  <spring:url value="/resources/layout/js/jquery.flot.min.js" var="jqueryflot_url" />
  <spring:url value="/resources/layout/js/jquery.flot.resize.min.js" var="jqueryflotrezise_url" />
